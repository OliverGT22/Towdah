            </div>
            <div>
                <groupbox
                    style="margin: 0 auto; border-radius: 5px; box-shadow: 1px 1px 1px"
                    closable="false" width="90%;" mold="3d">
                
                       
            <div sclass="row" >
                <div sclass="col-md-1">
           
                </div>
                <div sclass="col-md-10">
            
                    <div sclass="row" >
                        <div sclass="col-md-12 text-center">
                            <label value="Calendarizacion Citas" style="font-size:20px; font-weight:bold;"   />
                        </div>
                    </div>
                    <div id ="formulario" >
                                   
                 
                        <div sclass="row" >
                          
                            <div sclass="col-sm-4 col-md-2"></div>
                            <div sclass="col-sm-4 col-md-8">
                                <label value="ALUMNO " style="font-weight:bold;"  />
                                <combobox id="cbxAlu"  width="100%" value=""/>                         
                                
                            </div>
                        
                            <div sclass="col-sm-4 col-md-2"></div>
                        </div>
                    
                        <div sclass="row" >
                          
                            <div sclass="col-sm-4 col-md-2"></div>
                            <div sclass="col-sm-4 col-md-8">
                                <label value="INSTRUCTOR " style="font-weight:bold;"  />
                                <combobox id="cbxIns"  width="100%" value=""/>                         
                                
                            </div>
                        
                            <div sclass="col-sm-4 col-md-2"></div>
                        </div>
                        
                       
                    
                        <n:br/>
                    
                        <div sclass="row" >
                            <div sclass="col-md-3"/>
                             <div sclass="col-md-2">
                                    <button label="Guardar"  width="100%"  iconSclass="z-icon-search" id="btnGuardar" sclass="btn btn-primary btn-lg active" />
                                </div>
                                
                                <div sclass="col-md-2">
                                    <button label="Restaurar" width="100%"   iconSclass="z-icon-spinner" id="btnRest" sclass="btn btn-primary btn-lg active" />
                                </div>
                                
                                
                                                                    
                            <div sclass="col-md-3"/>
                        </div>
                    </div>  
                    <n:br/>
                    
                    <separator/>
                    <div sclass="row" >
                        <div sclass="col-md-1"/>
                        <div sclass="col-md-10">
                            <grid sclass="domain"  mold="paging"  pageSize="10"  id="dataGrid" emptyMessage="No Hay Boletas">
                                <columns>
                                    <column label="Id"  sort="auto"  width ="10%"/>
                                    <column label="Dia"  sort="auto"  width ="16%"/>
                                    <column label="Hora"   sort="auto" width ="16%" />
                                    <column label="Alumno"   sort="auto"  width ="16%"/>
                                    <column label="Instructor"   sort="auto"  width ="16%"/>
                                    <column label="Categoria"   sort="auto"  width ="16%"/>
                                </columns>
                                <rows id="rows">
                                </rows>
                            </grid>
                        </div>
                        <div sclass="col-md-1"/>
                    </div>
                </div>
                <div sclass="col-md-1">
          
                </div>
            </div>
  
                </groupbox>    

            </div>
            <div sclass="col-md-12 col-sm-12 row">
