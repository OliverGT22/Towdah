<zk xmlns:n="native">
    <div apply="Controles.CtrLCalendarizacionSR" sclass="col-md-12 col-sm-12">   
        <div id ="div1" sclass="col-md-12 col-sm-12 navbar" style="background-color: #872e01;">
            <!-- Brand Logo -->
            <n:br/>
            
            <hlayout>
                <vlayout>
                     <image sclass="img-responsive" src="/img/titulo.png" />
              
                </vlayout> 
            </hlayout>
            <n:br/>
            
            <vlayout>
               
                <hlayout>    
                    <label value="Volver al Login" id = "lbUser" width="100%" height="25px"  style="color:white;font-size: 1.8vw;"   /> 
                    <button iconSclass="z-icon-user" id = "btnlogint" width="100%" height="25px"  /> 
                </hlayout> 
            </vlayout>
            <n:br/>
            
        </div>
  
        <div   sclass="col-md-12 col-sm-12">

            <div sclass="col-md-12 col-sm-12 page-header text-center">       
